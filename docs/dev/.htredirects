# dev.perl.org redirections

/perl-styles.css	http://dev.perl.org/css/old-develooper.css

/rfc				        /perl6/rfc
/rfc/(.*)				/perl6/rfc/$1
/pdd					/perl6/pdd
/pdd/(.*)				/perl6/pdd/$1
/lists					/perl6/lists
/lists/(.*)				/perl6/lists/$1

/cvs                http://svn.perl.org/
/cvs/(.*)           http://svn.perl.org/

/parrot/?			http://www.parrotcode.org/

/perl6/architecture		/perl6/architecture.html
/perl6/code			/perl6/code.html
/perl6/people			/perl6/people.html
/perl6/status			/perl6/status.html

/perl6/doc(/(design/?)?)?       /perl6/architecture.html   perm
/perl6/doc/design/apo/?         /perl6/doc/apocalypse.html perm
/perl6/doc/design/syn/?         /perl6/doc/synopsis.html   perm
/perl6/doc/design/exe/?         /perl6/doc/exegesis.html   perm

/perl5/lists/?			/perl5/lists.html
/perl1/lists/?			/perl1/lists.html

/perl5/source/?			/perl5/source.html

/auth/(.*)			http://auth.perl.org/auth/$1	perm

# Whee
/perl6/synopsis/5.html /perl6/synopsis/S05.html

/perl6/apocalypse/(\d) /perl6/apocalypse/A0$1.html
/perl6/exegesis/(\d) /perl6/exegesis/E0$1.html

# moved stuff even more (4/2/05)
/perl6/apocalypse/?  /perl6/doc/apocalypse.html
/perl6/exegesis/?    /perl6/doc/exegesis.html
/perl6/synopsis/?    /perl6/doc/synopsis.html
/perl6/apocalypse/(A.*)  /perl6/doc/design/apo/$1
/perl6/exegesis/(E.*)    /perl6/doc/design/exe/$1
/perl6/synopsis/(S.*)    /perl6/doc/design/syn/$1


